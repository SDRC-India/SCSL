			$("ul.pageLinks.mainmenu > li a[href='#']").click(function() {
				if ($(this).next(".submenu").css("display") == 'none'){
					$(this).next(".submenu").slideDown("slow");
					$(this).addClass("opened");
					$(this).find("i.fa-chevron-down").css("transform", "rotate(-180deg)");
				}
				else{
					$(this).next(".submenu").slideUp("slow");
					$(this).removeClass("opened");
					$(this).find("i.fa-chevron-down").css("transform", "rotate(0deg)");
				}
					
			});
			var formInputFocused = false;
			$("div#loginPopForm input").focus(function(){
				formInputFocused = true;
				setTimeout(function(){
					if($(window).height()<=665 && $(window).width() <=1024){
						$("div#loginPopForm").css({
							"z-index": "9999",
							"top": "0",
							"transform": "translateY(0)"
						});
						formInputFocused = false;
	/* 					$(".loginPopBtn1").hide();
	 */				}	
				}, 100)	
			});
			$("div#loginPopForm input").blur(function(){
				setTimeout(function(){
					if($(window).height()<=665 && $(window).width() <=1024 && !formInputFocused){
						if($(window).height() <= 330){
							$("div#loginPopForm").css({
								"z-index": "9999",
								"top": "51px",
									"transform": "translateY(0)"
							})
						}
						else{
							$("div#loginPopForm").css({
								"z-index": "9999",
								"top": "75px",
									"transform": "translateY(0)"
							})
						}
						
						/* $(".loginPopBtn1").show(); */
					}	
				},100)
				
			});	
		});
	</script>
